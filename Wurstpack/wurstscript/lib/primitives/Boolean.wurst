package Boolean
import NoWurst

/** Converts a boolean into a string */
public function boolean.toString() returns string
	if this
		return "true"
	return "false"


public function boolean.toInt() returns int
	if this
		return 1
	else
		return 0

/** Exclusive Or */
public function boolean.xor(boolean other) returns boolean
	return (this or other) and !(this and other)
